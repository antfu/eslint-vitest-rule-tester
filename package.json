{
  "name": "eslint-vitest-rule-tester",
  "type": "module",
  "version": "2.2.2",
  "packageManager": "pnpm@10.17.1",
  "description": "ESLint rule tester with Vitest",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/eslint-vitest-rule-tester#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/eslint-vitest-rule-tester.git"
  },
  "bugs": "https://github.com/antfu/eslint-vitest-rule-tester/issues",
  "keywords": [
    "eslint",
    "vitest",
    "rule-tester"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "watch": "tsdown --watch",
    "lint": "eslint",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "vitest": "^1.0.0 || ^2.0.0 || ^3.0.0"
  },
  "dependencies": {
    "@types/eslint": "^9.6.1",
    "@typescript-eslint/utils": "^8.44.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@antfu/ni": "^26.0.1",
    "@antfu/utils": "^9.2.1",
    "@types/node": "^24.5.2",
    "@typescript-eslint/parser": "^8.44.1",
    "ajv": "^6.12.6",
    "bumpp": "^10.2.3",
    "eslint": "^9.36.0",
    "lint-staged": "^16.2.0",
    "simple-git-hooks": "^2.13.1",
    "taze": "^19.7.0",
    "tsdown": "^0.15.6",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
