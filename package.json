{
  "name": "eslint-vitest-rule-tester",
  "type": "module",
  "version": "1.1.0",
  "packageManager": "pnpm@10.6.2",
  "description": "ESLint rule tester with Vitest",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/eslint-vitest-rule-tester#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/eslint-vitest-rule-tester.git"
  },
  "bugs": "https://github.com/antfu/eslint-vitest-rule-tester/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "vitest": "^1.0.0 || ^2.0.0 || ^3.0.0"
  },
  "dependencies": {
    "@antfu/utils": "^9.1.0",
    "@types/eslint": "^9.6.1",
    "@typescript-eslint/utils": "^8.26.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.8.1",
    "@antfu/ni": "^24.1.0",
    "@types/node": "^22.13.10",
    "@typescript-eslint/parser": "^8.26.1",
    "ajv": "^6.12.6",
    "bumpp": "^10.1.0",
    "eslint": "^9.22.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.4.3",
    "pnpm": "^10.6.2",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "taze": "^19.0.2",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vite": "^6.2.1",
    "vitest": "^3.0.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
